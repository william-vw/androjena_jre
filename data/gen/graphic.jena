]

[G2:
	(<> xpl:output ?deriv) (?en xpl:derivation ?deriv) 
	(?en xpl:sourceObject ?o) isUri(?o) (?en xpl:sourcePredicate ?p)
	(?r a xpl:Resource) (?r rdf:predicate ?p) (?r rdf:object ?o) (?r xpl:img ?img)
